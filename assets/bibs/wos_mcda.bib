@article{ WOS:000464339600007,
Author = {Garau, Martina and Hampson, Grace and Devlin, Nancy and Mazzanti, Nicola
   Amedeo and Profico, Antonio},
Title = {Applying a Multicriteria Decision Analysis (MCDA) Approach to Elicit
   Stakeholders' Preferences in Italy: The Case of Obinutuzumab for
   Rituximab-Refractory Indolent Non-Hodgkin Lymphoma (iNHL)},
Journal = {PHARMACOECONOMICS-OPEN},
Year = {2018},
