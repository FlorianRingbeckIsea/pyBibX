   Romano, G. and Garzon-Orjuela, N. and Rosselli, D.},
Title = {International experiences in multicriteria decision analysis (MCDA) for
   evaluating orphan drugs: a scoping review},
Journal = {EXPERT REVIEW OF PHARMACOECONOMICS \& OUTCOMES RESEARCH},
Year = {2019},
