@article{ WOS:000378739000002,
Author = {Barocchi, Michele A. and Black, Steve and Rappuoli, Rino},
Title = {Multicriteria decision analysis and core values for enhancing
   vaccine-related decision-making},
Journal = {SCIENCE TRANSLATIONAL MEDICINE},
Year = {2016},
Volume = {8},
Number = {345},
Month = {JUN 29},
Abstract = {Vaccines have the potential to transform the health of all individuals
   and to reduce the health inequality between rich and poor countries.
   However, to achieve these goals, it is no longer sufficient to
   prioritize vaccine development using cost-effectiveness as the sole
   indicator. During a symposium entitled ``Mission Grand Convergence-The
   Role of Vaccines,{''} held in Siena, Italy, in July 2015, key
   stakeholders agreed that the prioritization of vaccine development and
   deployment must use multicriteria decision-making based on the following
   core concepts: (i) mortality and severity of the disease, (ii) vaccine
   safety considerations, and (iii) economic evaluation that captures the
   full benefits of vaccination.},
