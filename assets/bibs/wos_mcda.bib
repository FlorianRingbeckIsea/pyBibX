@article{ WOS:000348962900002,
Author = {Linkov, Igor and Massey, Olivia and Keisler, Jeff and Rusyn, Ivan and
   Hartung, Thomas},
Title = {From ``Weight of Evidence{''} to Quantitative Data Integration using
   Multicriteria Decision Analysis and Bayesian Methods},
Journal = {ALTEX-ALTERNATIVES TO ANIMAL EXPERIMENTATION},
