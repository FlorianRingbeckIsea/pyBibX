@article{ WOS:000266451900007,
Author = {Corsair, H. J. and Ruch, Jennifer Bassman and Zheng, Pearl Q. and Hobbs,
   Benjamin F. and Koonce, Joseph F.},
Title = {Multicriteria Decision Analysis of Stream Restoration: Potential and
   Examples},
