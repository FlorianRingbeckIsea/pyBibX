   and Huang, Shan-Lin},
Title = {Multicriteria Decision Analysis to Develop Effective Sustainable
   Development Strategies for Enhancing Competitive Advantages: Case of the
   TFT-LCD Industry in Taiwan},
Journal = {SUSTAINABILITY},
