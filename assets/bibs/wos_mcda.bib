@inproceedings{ WOS:000288622300105,
Author = {Ugur, Ibrahim and Sener, Erhan},
Book-Group-Author = {SGEM},
Title = {SITE SELECTION FOR MARBLE WASTES USING MULTICRITERIA DECISION ANALYSIS
   AND GEOGRAPHIC INFORMATION SYSTEMS: A CASE STUDY IN BUCAK (BURDUR,
   TURKEY)},
Booktitle = {10TH INTERNATIONAL MULTIDISCIPLINARY SCIENTIFIC GEOCONFERENCE: SGEM
   2010, VOL II},
