   Santos, Eduardo A. P.},
Editor = {Peruzzini, M and Pellicciari, M and Bil, C and Stjepandic, J and Wognum, N},
Title = {Industry 4.0 Through Organizational Interoperability Perspective: A
   Multicriteria Decision Analysis},
Booktitle = {TRANSDISCIPLINARY ENGINEERING METHODS FOR SOCIAL INNOVATION OF INDUSTRY
   4.0},
Series = {Advances in Transdisciplinary Engineering},
