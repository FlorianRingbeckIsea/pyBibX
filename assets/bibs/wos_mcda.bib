@article{ WOS:000265153700004,
Author = {Hossain, M. Shahadat and Chowdhury, Sayedur Rahman and Das, Nani Gopal
   and Sharifuzzaman, S. M. and Sultana, Abida},
Title = {Integration of GIS and multicriteria decision analysis for urban
   aquaculture development in Bangladesh},
Journal = {LANDSCAPE AND URBAN PLANNING},
Year = {2009},
Volume = {90},
Number = {3-4},
Pages = {119-133},
Month = {APR 30},
Abstract = {Site selection is a key factor in any aquaculture operation, affecting
   both success and sustainability as well as solving land or water use
   conflicts. This study was conducted to identify suitable sites for carp
   farming development in urban water bodies (UWBs) of Chittagong,
   Bangladesh using Geographical Information Systems (GIS) based
   MultiCriteria Evaluation (MCE) of water, soil and infrastructure
   database. ASTER imagery and 14 thematic layers were analyzed with ENVI
   and CIS capabilities, and developed a series of CIS models to identify
   and prioritize the appropriate UWBs for carp farming. The Study
   identified 487 UWBs occupying 362 ha and revealed 280 ha (77\%) is the
   most suitable, 36 ha (10\%) is moderately suitable and 46 ha (13\%) is
   not suitable which was consistent with field verification. The results
   are encouraging for extension of carp culture and diversify the economic
   activities of the urban dwellers. (C) 2008 Elsevier B.V. All rights
   reserved.},
