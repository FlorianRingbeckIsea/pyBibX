@article{ WOS:000521256600102,
Author = {Fusade-Boyer, Maxime and Pato, Pidemnewe S. and Komlan, Mathias and
   Dogno, Koffi and Batawui, Komla and Go-Maro, Emilie and McKenzie, Pamela
   and Guinat, Claire and Secula, Aurelie and Paul, Mathilde and Webby,
   Richard J. and Tran, Annelise and Waret-Szkuta, Agnes and Ducatez,
   Mariette F.},
Title = {Risk Mapping of Influenza D Virus Occurrence in Ruminants and Swine in
   Togo Using a Spatial Multicriteria Decision Analysis Approach},
Journal = {VIRUSES-BASEL},
Year = {2020},
Volume = {12},
Number = {2},
Month = {FEB},
Abstract = {Influenza D virus (IDV) has been identified in several continents, with
   serological evidence for the virus in Africa. In order to improve the
   sensitivity and cost-benefit of IDV surveillance in Togo, risk maps were
   drawn using a spatial multicriteria decision analysis (MCDA) and
   experts' opinion to evaluate the relevance of sampling areas used so
   far. Areas at highest risk of IDV occurrence were the main cattle
   markets. The maps were evaluated with previous field surveillance data
   collected in Togo between 2017 and 2019: 1216 sera from cattle, small
   ruminants, and swine were screened for antibodies to IDV by
   hemagglutination inhibition (HI) assays. While further samples
   collections are needed to validate the maps, the risk maps resulting
   from the spatial MCDA approach generated here highlight several priority
   areas for IDV circulation assessment.},
