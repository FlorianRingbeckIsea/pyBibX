@article{ WOS:000395153100001,
Author = {Myagmartseren, Purevtseren and Buyandelger, Myagmarsuren and Brandt, S.
   Anders},
Title = {Implications of a Spatial Multicriteria Decision Analysis for Urban
   Development in Ulaanbaatar, Mongolia},
Journal = {MATHEMATICAL PROBLEMS IN ENGINEERING},
Year = {2017},
Volume = {2017},
Abstract = {New technology has provided new tools for effective spatial planning.
   Through the example of locating suitable sites for urban development in
   Ulaanbaatar, this paper illustrates how multicriteria decision analysis
   and geographical information systems can be used for more effective
   urban planning. Several constraint and factor criteria were identified,
   transformed into map layers, and weighted together using the analytic
   hierarchy process. Besides localization results, this study shows the
   effect of using poor elevation data and how a sensitivity analysis can
   be applied to yield further information, spot weighting weaknesses, and
   assess the quality of the criteria.},
Publisher = {HINDAWI LTD},
Address = {ADAM HOUSE, 3RD FLR, 1 FITZROY SQ, LONDON, W1T 5HF, ENGLAND},
Type = {Article},
